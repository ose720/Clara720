class Test {
  static void align(int a[]) {
      int temp;
      for(int i = 0; i<4;i++ ) {
        for(int j = 0; j < 4-i; j++) {
        if(a[j] > a[j+1]) {
        temp = a[j];
      }
    }
  }
}

public static void main(String[] args) {
  int a[] = {8, 3, 4, 9, 7};
  align(a);
  
  for(int i = 0; i<5 ;i++) {
      System.out.print(a[i]+ " ");
      }
    }
  }
